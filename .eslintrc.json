{
    
    
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {"react/prop-types": "off"}
  }